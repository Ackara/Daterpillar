<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Acklann.Daterpillar</RootNamespace>
    <AssemblyName>Acklann.Daterpillar</AssemblyName>
  </PropertyGroup>
  <PropertyGroup>
    <Title>Daterpillar</Title>
    <PackageVersion>7.0.2</PackageVersion>
    <AssemblyVersion>7.0.2</AssemblyVersion>
    <Authors>Ackara</Authors>
    <Company>Ackara</Company>
    <Owners>Ackara</Owners>
    <Copyright>Copyright © 2015 Ackara</Copyright>
    <RepositoryUrl>https://github.com/Ackara/Daterpillar.git</RepositoryUrl>
    <PackageProjectUrl>https://github.com/Ackara/Daterpillar</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/Ackara/Daterpillar/blob/master/license.txt</PackageLicenseUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/Ackara/Daterpillar/master/art/icon.png</PackageIconUrl>
    <Description>A mico orm</Description>
    <PackageTags>sql migration orm flyway build</PackageTags>
    <PackageReleaseNotes>https://github.com/Ackara/Daterpillar/blob/master/releaseNotes.txt</PackageReleaseNotes>
    <RunPostBuildEvent>OnOutputUpdated</RunPostBuildEvent>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Acklann.GlobN" Version="2.1.7" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="daterpillar.xsd" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Configuration\Language.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
    <Compile Update="Translators\TranslatorBase.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <Service Include="{508349b6-6b84-4df5-91f0-309beebad82d}" />
  </ItemGroup>
  <ItemGroup Label="Nuget">
    <Content Include="..\..\readme.md">
      <PackagePath>readme.md</PackagePath>
      <Visible>false</Visible>
    </Content>
    <Content Include="msbuild.targets">
      <PackagePath>build\$(AssemblyName).targets</PackagePath>
    </Content>
    <Content Condition="Exists('..\..\artifacts\$(MSBuildProjectName)')" Include="..\..\artifacts\$(MSBuildProjectName)\*">
      <PackagePath>tools\$(TargetFramework)</PackagePath>
      <Visible>false</Visible>
    </Content>
    <Content Include="..\$(MSBuildProjectName).Powershell\MSBuild\*.ps1">
      <PackagePath>tools\$(TargetFramework)</PackagePath>
      <Visible>false</Visible>
    </Content>
    <Content Include="*.xsd">
      <PackagePath>tools\$(TargetFramework)</PackagePath>
    </Content>
  </ItemGroup>
</Project>